{
  "name": "freshworkx/oauth2-thingiverse",
  "description": "Thingiverse OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
  "license": "MIT",
  "authors": [
    {
      "name": "Jens Neumann",
      "email": "info@freshworkx.de",
      "homepage": "https://www.freshworkx.de/"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authorisation",
    "thingiverse"
  ],
  "require": {
    "php": "^5.6 || ^7.0 || ^8.0",
    "league/oauth2-client": "^2.4"
  },
  "require-dev": {
    "ext-json": "*",
    "phpunit/phpunit": "~9.5",
    "mockery/mockery": "~1.4",
    "squizlabs/php_codesniffer": "~3.5"
  },
  "autoload": {
    "psr-4": {
      "Freshworkx\\OAuth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Freshworkx\\OAuth2\\Client\\Test\\": "tests/src/"
    }
  }
}